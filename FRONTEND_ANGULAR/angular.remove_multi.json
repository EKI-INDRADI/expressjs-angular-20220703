{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "rnd-appv2": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/rnd-appv2",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": false,
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css",
              "src/assets/frontend-files/css-files/dashboard/css/sb-admin-2.min.css",
              "src/assets/frontend-files/css-files/dashboard/css/sb-admin-2.css",
              "https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i",
              "src/assets/frontend-files/css-dashboard/vendor/datatables/dataTables.bootstrap4.min.css"
            ],
            "scripts": [
              "src/assets/frontend-files/css-files/dashboard/vendor/jquery/jquery.min.js",
              "src/assets/frontend-files/css-files/dashboard/vendor/bootstrap/js/bootstrap.bundle.min.js",
              "src/assets/frontend-files/css-files/dashboard/vendor/jquery-easing/jquery.easing.min.js",
              "src/assets/frontend-files/css-files/dashboard/js/sb-admin-2.min.js",
              "src/assets/frontend-files/css-files/dashboard/vendor/datatables/jquery.dataTables.min.js",
              "src/assets/frontend-files/css-files/dashboard/vendor/datatables/dataTables.bootstrap4.min.js",
              "src/assets/frontend-files/css-files/dashboard/js/demo/datatables-demo.js",
              "src/assets/frontend-files/css-js-eki-files/setup_js/fe-all/fe-all-preview-img.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "rnd-appv2:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "rnd-appv2:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "rnd-appv2:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "rnd-appv2:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "rnd-appv2:serve:production"
            }
          }
        }
      }
    }},
  "defaultProject": "rnd-appv2"
}